  "keywords": [
    "components",
    "ui",
    "styled",
    "tempestphp",
    "tempest",
    "vewe"
  ],
  "homepage": "https://github.com/iamdadmin/vewe",
  "support": {
    "issues": "https://github.com/iamdadmin/vewe/issues",
    "source": "https://github.com/iamdadmin/vewe/"
  },
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "iamdadmin",
      "email": "no@email.yet"
    }
  ],
  "require": {
    "php": "^8.5",
    "feature-ninja/cva": "^0.3",
