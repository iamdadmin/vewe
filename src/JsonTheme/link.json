{
    "base": "focus-visible:outline-primary",
    "variants": {
        "active": {
            "true": {
                "base": "text-primary"
            },
            "false": {
                "base": "text-muted"
            }
        },
        "disabled": {
            "true": {
                "base": "cursor-not-allowed opacity-75"
            }
        }
    },
    "compoundVariants": [
        {
            "active": false,
            "disabled": false,
            "class": [
                "hover:text-default",
                "(options.theme.transitions) && transition-colors"
            ]
        }
    ]
}